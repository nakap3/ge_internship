namespace Data;

enum Hand : short { stone, paper, scissors }

table FbMonsterData
{
  label:string;
  name:string;
  hp:short;
  ap:short;
  dp:short;
  hand:Hand;
}

table FbMonsterList
{
  monster_list:[FbMonsterData];
}

root_type FbMonsterList;


file_identifier "MDFB";
file_extension "mdfb";
